{
    initial_position    16 160
    scroll_speed        -0.004 -0.004
    behavior            LINEAR -0.16 0
    repeat_x            TRUE
    repeat_y            FALSE

    sprite
    {
        source_file     "images/backgrounds/waterworks-zone/water-mask.png"
        source_rect     0 16 256 16
        frame_size      256 16

        animation
        {
            repeat      TRUE
